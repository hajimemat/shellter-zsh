# ============================================================
# Zshのローカル設定
# ============================================================

# パスを追加
path=(
	~/bin
	~/go
	$path
)

export EDITOR=nvim
alias vi=nvim
alias vim=nvim
alias vimdiff='nvim -d'

[ -f ~/.zsh/functions/fzf.sh ] && source ~/.zsh/functions/fzf.sh
